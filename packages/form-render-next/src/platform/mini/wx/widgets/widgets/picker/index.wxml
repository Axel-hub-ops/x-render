<wxs
  module="__dp_f"
  src="../../../../../../../../npm_modules/@ali/dslplus-mini-compiler-helper/safescript/property-type.wxs"
></wxs>
<view
  class="pickerTips"
  bindtap="%bind%"
  data-::dp:a:ontap="openPicker"
>当前选择的元素：
  <picker
    data-:dp:a="{{({ className: __dp_f(className), value: __dp_f(value), data: __dp_f(pickerList) })}}"
    data-:dp:b="{{$id}}"
    :cols="{{3}}"
    :className="{{className}}"
    :disabled="{{false}}"
    :onFormat="onFormat"
    :onDismiss="onDismiss"
    :onOk="onOk"
    :value="{{value}}"
    :onChange="onChange"
    :pickerholder="请选择"
    :data="{{pickerList}}"
  >
    <view slot="title">Picker选择器</view>
  </picker>
</view>